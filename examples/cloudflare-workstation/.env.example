# Copy this file to .env and fill in values

# Wrangler authentication (for deploys)
CLOUDFLARE_API_TOKEN=
# Optional legacy depending on org setup
CLOUDFLARE_EMAIL=

# Worker runtime vars
GREETING=Hello from Cloudflare Workers

# Resource configuration
# Use names where Wrangler can resolve resources by name within your account.
# KV requires a namespace ID; obtain via `wrangler kv namespace list`.

# D1
D1_DATABASE_NAME=localdb

# R2
R2_BUCKET_NAME=local-bucket

# KV
KV_NAMESPACE_ID=00000000000000000000000000000000

